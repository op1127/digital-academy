{
  "name": "digital-academy",
  "private": true,
  "description": "An online tech academy",
  "version": "0.1.0",
  "author": "Omar Plasencia",
  "dependencies": {
    "gatsby": "^2.15.29",
    "gatsby-image": "^2.2.24",
    "gatsby-plugin-manifest": "^2.2.20",
    "gatsby-plugin-offline": "^3.0.11",
    "gatsby-plugin-react-helmet": "^3.1.10",
    "gatsby-plugin-sharp": "^2.2.28",
    "gatsby-source-filesystem": "^2.1.29",
    "gatsby-transformer-sharp": "^2.2.20",
    "npm-run-all": "^4.1.5",
    "prop-types": "^15.7.2",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-helmet": "^5.2.1"
  },
  "devDependencies": {
    "postcss": "^7.0.18",
    "postcss-cli": "^6.1.3",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^1.18.2"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "watch:sass": "sass src/styles/styles.scss src/styles/styles.css --watch",
    "prefix:css": "postcss src/styles/styles.css -o src/styles/styles.prefix.css -w",
    "build": "gatsby build",
    "develop": "gatsby develop",
    "dev": "npm-run-all --parallel watch:sass prefix:css develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing \""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
